  "configuration": {
    "tagName": "noi-automotive",
    "options": [
      {
        "key": "language",
        "type": "select",
        "label": "language",
        "required": false,
        "options": {
          "values": ["en", "de", "it"],
          "default": "en"
        }
      },
      {
        "key": "website-mode",
        "type": "text",
        "label": "Website Mode",
        "required": false,
        "options": {
          "default": "false"
        }
      },
      {
        "key": "default-company",
        "type": "text",
        "label": "Default Company",
        "required": false,
        "options": {
          "default": ""
        }
      },
      {
        "key": "default-category",
        "type": "select",
        "label": "Filter category at startup",
        "required": false,
        "options": {
          "values": [
            "",
            "automotiveAndMobility",
            "manufacturing",
            "agriAutomation"
          ]
        }
      },
      {
        "key": "visible-categories",
        "type": "multiselect",
        "label": "Only show selected categories",
        "required": false,
        "options": {
          "values": [
            "",
            "automotiveAndMobility",
            "manufacturing",
            "agriAutomation"
          ]
        }
      },
      {
        "key": "limit-to-default-category",
        "type": "null",
        "label": "Only display default category",
        "required": false,
        "options": {
          "default": false
        }
      },
      {
        "key": "custom-links",
        "type": "text",
        "label": "Custom Links shown in footer",
        "required": false,
        "options": {
          "default": ""
        }
      },
      {
        "key": "primary-color",
        "type": "text",
        "label": "Primary color (Hex Code)",
        "required": false,
        "options": {
          "default": "#0000ff"
        }
      },
      {
        "key": "width",
        "type": "text",
        "label": "Width of Webcomponent (CSS)",
        "required": false,
        "options": {
          "default": "100%"
        }
      },
      {
        "key": "height",
        "type": "text",
        "label": "Height of Webcomponent (CSS)",
        "required": false,
        "options": {
          "default": "100%"
        }
      },
      {
        "key": "show-home-view",
        "type": "bool",
        "label": "Show Home View?",
        "required": false,
        "options": {
          "default": true
        }
      }
    ]
  }
